
@{
  
    Layout = "~/Views/Shared/myLayOut.cshtml";
}
<h1>In-Active Vendors</h1>
<div class="row" id="products_date">

    <div class="col-md-12">




        <table id="myTable">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Email</th>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col">Req Date</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                @{var a = 0; }
                @foreach (var item in ViewBag.vendorlistActive)
                {

                    <tr>

                        <td>@item.userID</td>
                        <td>@item.email</td>
                        <td>@item.first_name</td>
                        <td>@item.last_name</td>
                        <td>@item.create_at</td>


                        <td>
                            <div class="btn-group" role="group" aria-label="Basic example">

                                <a  href="@Url.Action("Approve", "controller",new { id = item.userID },null)" class="btn btn-primary update_product">Approve</a>

                            </div>


                        </td>

                    </tr>

                }
            </tbody>
        </table>


    </div>
</div>

<script>

    $(document).ready(function () {


        $(".side_nave_hide").remove();


    })

</script>
